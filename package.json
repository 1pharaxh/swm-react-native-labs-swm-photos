{
  "name": "images-gallery",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.2.0"
  },
  "scripts": {
    "start": "expo start",
    "prebuild": "npx expo prebuild",
    "ios:release": "BUILD_TYPE=release expo run:ios --configuration Release -d",
    "ios": "BUILD_TYPE=dev expo run:ios -d",
    "android:release": "BUILD_TYPE=release expo run:android --variant release -d",
    "android": "BUILD_TYPE=dev expo run:android -d",
    "prebuild:tv": "EXPO_TV=1 npx expo prebuild",
    "tvos:release": "BUILD_TYPE=release EXPO_TV=1 expo run:ios --configuration Release -d",
    "tvos": "BUILD_TYPE=dev EXPO_TV=1 expo run:ios -d",
    "androidtv:release": "BUILD_TYPE=release EXPO_TV=1 expo run:android --variant release -d",
    "androidtv": "BUILD_TYPE=dev EXPO_TV=1 expo run:android -d",
    "web:release": "BUILD_TYPE=release expo start --web -c",
    "web": "BUILD_TYPE=dev expo start --web -c",
    "lint": "expo lint",
    "tsc": "tsc"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@shopify/flash-list": "2.0.2",
    "expo": "^54.0.12",
    "expo-constants": "~18.0.9",
    "expo-file-system": "~19.0.16",
    "expo-font": "~14.0.8",
    "expo-image": "~3.0.0",
    "expo-image-manipulator": "~14.0.7",
    "expo-linking": "~8.0.8",
    "expo-media-library": "~18.2.0",
    "expo-router": "~6.0.10",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "metro": "^0.83.1",
    "p-limit": "^6.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-logs": "^5.3.0",
    "react-native-pixel-perfect": "^1.0.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.5.0",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-tvos/config-tv": "^0.1.3",
    "@types/cli-progress": "^3.11.6",
    "@types/react": "~19.1.10",
    "cli-progress": "^3.12.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "inquirer": "12.6.3",
    "prettier": "^3.5.3",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "android": {
        "buildFromSource": [
          "expo-image"
        ]
      }
    }
  }
}
