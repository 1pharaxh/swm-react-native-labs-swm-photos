# Copy this file to .env and customize with your own values
# This ensures each developer has a unique bundle identifier

# iOS Bundle Identifier (replace with your unique identifier)
EXPO_BUNDLE_IDENTIFIER=com.swmansion.photos

# Android Package Name (optional, for consistency)
EXPO_ANDROID_PACKAGE=com.swmansion.photos

# Enable production mode: skip the menu and launch the `photos-gallery` scenario directly
EXPO_PUBLIC_LAUNCH_GALLERY_ON_START=true
